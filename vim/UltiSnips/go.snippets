# table test
snippet tt
var testCases = []struct {
	name string
	given string
	expect string
}{
	{
		name: "${1}",
		given: "${2}",
		expect: "${3}",
	},
}
for _, tc := range testCases {
	tc := tc
	t.Run(tc.name, func(t *testing.T){
		got := ${0:${VISUAL}}(tt.given)

		if got != tc.expect {
				t.Errorf("$0(%s): expected %s, got %s", tc.given, tc.expect, got)
		}
	})
}
endsnippet
